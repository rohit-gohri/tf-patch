{
  "name": "tf-patch",
  "description": "Patch terraform modules to fix tiny things",
  "version": "0.0.0",
  "bin": "./bin/run.js",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "./bin/dev.js",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "tf-patch",
    "dirname": "tf-patch",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "patch": {
        "description": "Copy terraform module so patch can be generated from changes"
      },
      "commit": {
        "description": "Generate .patch file that can be committed to .patches folder"
      },
      "init": {
        "description": "Apply patches to downloaded terraform modules"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-autocomplete": "^3.2.0",
    "@oclif/plugin-help": "^6",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "lodash-es": "^4.17.21",
    "parse-diff": "^0.11.1",
    "tmp": "^0.2.3"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/fs-extra": "^11",
    "@types/lodash-es": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/tmp": "^0",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "author": "Rohit Gohri @rohit-gohri",
  "bugs": "https://github.com/rohit-gohri/terraform-patch/issues",
  "homepage": "https://github.com/rohit-gohri/terraform-patch",
  "license": "MIT",
  "keywords": [
    "terraform",
    "patch-package",
    "tf-modules",
    "patch-modules",
    "cli",
    "oclif"
  ],
  "repository": "rohit-gohri/terraform-patch",
  "packageManager": "yarn@4.4.0"
}
